account robert
host send.one.com
port 587
protocol smtp
from robert@dahl-jacobsen.dk
user robert@dahl-jacobsen.dk
passwordeval "gpg2 -q --for-your-eyes-only --no-tty -d ~/.mutt/accounts/robert.gpg"
tls on
tls_starttls on
tls_certcheck off
auth login

