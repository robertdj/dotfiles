# MS Word documents
application/msword; lowriter %s

# MS Excel
application/vnd.openxmlformats-officedocument.wordprocessingml.document; lowriter %s

application/pdf; evince %s

# Display html emails inline
text/html; elinks -dump -dump-charset utf8 -default-mime-type text/html %s; needsterminal; copiousoutput;

