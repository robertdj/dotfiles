{
    "highlight_selected_line": true,
    "panes": {
        "quadrants": [
            "Source",
            "TabSet2",
            "TabSet1",
            "Console"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "Files",
            "Plots",
            "Help",
            "Build",
            "VCS",
            "Presentation"
        ],
        "tabSet2": [
            "Connections",
            "Packages",
            "Tutorial",
            "Viewer"
